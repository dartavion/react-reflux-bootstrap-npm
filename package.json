{
  "name": "react-reflux-bootstrap-npm",
  "version": "1.0.0",
  "description": "build react-reflux-bootstrap with npm",
  "main": "index.js",
  "scripts": {
    "start": "node build/server/server",
    "build-site": "npm run clean && npm run copy-scripts && npm run build-all-js && npm run copy-assets && npm run clean-tmp && npm start",
    "clean": "rimraf build tmp",
    "clean-tmp": "rimraf tmp",
    "copy-json": "mkdir -p build/client/json && copyfiles -f src/client/app/json/*.json build/client/json",
    "copy-html": "copyfiles -f src/client/**/*.html build/client",
    "copy-styles": "mkdir -p build/client/styles && npm run build-css",
    "copy-scripts": "mkdir -p build/client/scripts",
    "copy-images": "copyfiles -f src/client/image/**/* build/client/images",
    "copy-fonts": "copyfiles -f src/client/fonts/**/* build/client/fonts",
    "copy-favicon": "copyfiles -f src/server/**/*.ico build/server",
    "copy-assets": "npm run copy-html && npm run copy-styles && npm run copy-scripts && npm run copy-images && npm run copy-fonts && npm run copy-favicon && npm run copy-json",
    "copy": "npm run copy-assets",
    "build-coffee": "coffee -o tmp -c src/client",
    "build-server-coffee": "coffee -o build/server -c src/server",
    "build-cjsx": "cjsx -o tmp -c src",
    "build-all-js": "npm run build-server-coffee && npm run build-coffee && npm run build-cjsx && npm run browserify-js",
    "browserify-js": "browserify tmp/client/app.js -o build/client/scripts/app.js",
    "build-css": "myth src/client/app.css build/client/styles/app.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dartavion/react-reflux-bootstrap-npm.git"
  },
  "keywords": [
    "react",
    "reflux",
    "bootstrap",
    "npm",
    "build"
  ],
  "author": "richard ranke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dartavion/react-reflux-bootstrap-npm/issues"
  },
  "dependencies": {
    "MD5": "^1.2.1",
    "body-parser": "^1.12.2",
    "bootstrap": "^3.3.4",
    "browserify": "^9.0.8",
    "clean-css": "^3.1.9",
    "coffee-react": "^3.0.1",
    "coffee-react-transform": "^3.0.1",
    "coffee-script": "^1.9.1",
    "concat-cli": "^1.0.1",
    "copyfiles": "^0.1.0",
    "express": "^4.12.3",
    "font-awesome": "^4.3.0",
    "fs-extra": "^0.18.0",
    "jscat": "0.0.2",
    "less": "^2.5.0",
    "lodash": "^3.6.0",
    "mkdir": "0.0.2",
    "morgan": "^1.5.2",
    "morris.js": "^0.5.0",
    "myth": "^1.4.0",
    "newforms": "^0.12.1",
    "node-cjsx": "^1.0.0",
    "react": "^0.13.1",
    "react-bootstrap": "^0.20.3",
    "react-responsive": "0.0.6",
    "react-router": "^0.13.2",
    "react-stats": "0.0.5",
    "react-swipe": "^2.0.9",
    "reactify": "^1.1.0",
    "reflux": "^0.2.7",
    "rework-npm-cli": "^0.1.1",
    "rimraf": "^2.3.2",
    "serve-favicon": "^2.2.0",
    "superagent": "^1.2.0",
    "watchify": "^3.1.1"
  }
}
